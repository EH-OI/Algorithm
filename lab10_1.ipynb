   "source": [
    "def fibonacci(n):\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    elif n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        #n>=2 라면 \n",
    "        return fibonacci(n-1) + fibonacci(n-2) #재귀적으로 F(n-1)+F(n-2) 계산 \n",
    "\n",
    "n = int(input(\"입력 : \"))\n",
    "\n",
    "for i in range(n + 1):\n",
    "    print(f\"Fibonacci({i}) : {fibonacci(i)}\")"
   ]
